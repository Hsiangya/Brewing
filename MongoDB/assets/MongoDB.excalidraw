    {
      "type": "text",
      "version": 1162,
      "versionNonce": 121554881,
      "isDeleted": false,
      "id": "XByfwFl4x1pMzVWGixgkU",
      "fillStyle": "hachure",
      "strokeWidth": 1,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "angle": 0,
      "x": 1513.5586961643817,
      "y": 2256.531242814699,
      "strokeColor": "#000000",
      "backgroundColor": "transparent",
